   "source": [
    "#find all numbers divisible by 7, but not multiple of 5 b/w 2000 and 3200 (included), numbers should be printed\n",
    "#in a comma separated sequence in a single line\n",
    "\n",
    "listnum = []\n",
    "for x in range(2000, 3201):\n",
    "    if x%7==0 and x%5!=0:\n",
    "        listnum.append(str(x))\n",
    "print(', '.join(listnum))"
   ]
  }
